  LIBS += -lgtest_main -lgtest
  LDDEPS +=
  ALL_LDFLAGS += $(LDFLAGS) -Llibs/gtests/Debug -L/usr/lib64 -m64
  LINKCMD = $(CXX) -o "$@" $(OBJECTS) $(RESOURCES) $(ALL_LDFLAGS) $(LIBS)
  define PREBUILDCMDS
  endef
